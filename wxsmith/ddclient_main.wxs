<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="ddclient_main">
		<size>776,383</size>
		<id_arg>0</id_arg>
		<object class="wxNotebook" name="ID_NOTEBOOK1" variable="Notebook1" member="yes">
			<pos>88,48</pos>
			<object class="notebookpage">
				<object class="wxPanel" name="ID_PANEL1" variable="Panel1" member="yes">
					<object class="wxBoxSizer" variable="BoxSizer1" member="no">
						<object class="sizeritem">
							<object class="wxBoxSizer" variable="BoxSizer4" member="no">
								<orient>wxVERTICAL</orient>
								<object class="sizeritem">
									<object class="wxGrid" name="ID_GRID1" variable="Grid1" member="yes">
										<cols>6</cols>
										<collabels>
											<item>ID</item>
											<item>Added</item>
											<item>Title</item>
											<item>URL</item>
											<item>Status</item>
											<item>Progress</item>
										</collabels>
										<style>wxVSCROLL|wxHSCROLL</style>
									</object>
									<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
									<border>5</border>
									<option>1</option>
								</object>
							</object>
							<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
							<border>5</border>
							<option>1</option>
						</object>
					</object>
				</object>
				<label>All Downloads</label>
			</object>
			<object class="notebookpage">
				<object class="wxPanel" name="ID_PANEL2" variable="Panel2" member="yes">
					<object class="wxBoxSizer" variable="BoxSizer2" member="no">
						<object class="sizeritem">
							<object class="wxListCtrl" name="ID_LISTCTRL1" variable="ListCtrl1" member="yes" />
							<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
							<border>5</border>
							<option>1</option>
						</object>
					</object>
				</object>
				<label>Running Downloads</label>
			</object>
			<object class="notebookpage">
				<object class="wxPanel" name="ID_PANEL3" variable="Panel3" member="yes">
					<object class="wxBoxSizer" variable="BoxSizer3" member="no" />
				</object>
				<label>Finished Downloads</label>
			</object>
		</object>
		<object class="wxMenuBar" variable="MenuBar1" member="yes" />
		<object class="wxToolBar" name="ID_TOOLBAR1" variable="ToolBar1" member="yes">
			<object class="tool" name="connect_btn" variable="ToolBarItem1" member="yes">
				<label>Connect</label>
				<bitmap stock_id="wxART_EXECUTABLE_FILE" stock_client="wxART_TOOLBAR" />
				<tooltip>Connect to a DownloadDaemon</tooltip>
				<handler function="OnConnectBtnClicked" entry="EVT_TOOL" />
			</object>
			<object class="separator" />
			<object class="tool" name="add_btn" variable="ToolBarItem2" member="yes">
				<label>Add</label>
				<bitmap stock_id="wxART_ADD_BOOKMARK" stock_client="wxART_TOOLBAR" />
				<tooltip>Adds a new Download</tooltip>
			</object>
			<object class="tool" name="del_btn" variable="ToolBarItem3" member="yes">
				<label>Delete</label>
				<bitmap stock_id="wxART_DEL_BOOKMARK" stock_client="wxART_TOOLBAR" />
				<tooltip>Deletes the selected Download</tooltip>
			</object>
			<object class="separator" />
			<object class="tool" name="stop_btn" variable="ToolBarItem4" member="yes">
				<label>Stop</label>
				<bitmap stock_id="wxART_CROSS_MARK" stock_client="wxART_TOOLBAR" />
				<tooltip>Stop downloading files</tooltip>
			</object>
			<object class="tool" name="start_btn" variable="ToolBarItem5" member="yes">
				<label>Start</label>
				<bitmap stock_id="wxART_TICK_MARK" />
				<tooltip>Start downloading files</tooltip>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-10</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
	</object>
</wxsmith>
